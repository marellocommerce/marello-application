{
    "name": "marellocommerce/marello",
    "description": "Marello Unified Commerce Management",
    "homepage": "https://github.com/marellocommerce/marello.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Marello B.V.",
            "homepage": "https://www.marello.com"
        }
    ],
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "require": {
        "oro/platform": "4.1.*",
        "oro/platform-serialised-fields": "4.1.*",
        "oro/calendar-bundle": "4.1.*",
        "mpdf/mpdf": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "autoload": {
        "psr-4": {"": "src/"},
        "exclude-from-classmap": ["/Tests/"]
    }
}
