{
    "name": "marellocommerce/marello-application-ee",
    "description": "Marello Unified Commerce Management Enterprise Edition",
    "homepage": "https://github.com/marellocommerce/marello-application-ee.git",
    "license": "Commercial",
    "authors": [
        {
            "name": "Marello B.V.",
            "homepage": "https://www.marello.com"
        }
    ],
    "autoload": {
        "psr-4": {"": "src/"},
        "files": ["src/AppKernel.php"],
        "exclude-from-classmap": ["**/Tests/"]
    },
    "repositories": {
        "health-check-bundle": {
            "type": "vcs",
            "url": "https://github.com/oroinc/OroHealthCheckBundle.git"
        },
        "composer": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "platform-enterprise": {
            "type": "vcs",
            "url": "https://github.com/oroinc/platform-enterprise.git"
        },
        "platform": {
            "type": "vcs",
            "url": "https://github.com/oroinc/platform-er.git"
        },
        "marello-enterprise": {
            "type": "vcs",
            "url": "https://github.com/marellocommerce/marello-enterprise.git"
        },
        "marello": {
            "type": "vcs",
            "url": "https://github.com/marellocommerce/marello.git"
        }
    },
    "require": {
        "oro/platform-enterprise": "5.1.*",
        "oro/platform-serialised-fields": "5.1.*",
        "oro/calendar-bundle": "5.1.*",
        "oro/health-check-bundle": "5.1.*",
        "oro/oauth2-server": "5.1.*",
        "oro/crm-task-bundle": "5.1.*",
        "marellocommerce/marello-enterprise": "~5.0.0"
    },
    "require-dev": {
        "behat/behat": "~3.10.0",
        "behat/gherkin": "~4.9.0",
        "behat/mink": "~1.9.0",
        "friends-of-behat/mink-extension": "~v2.5.0",
        "behat/mink-selenium2-driver": "~1.5.0",
        "friends-of-behat/symfony-extension": "~2.2.0",
        "friendsofphp/php-cs-fixer": "~3.4.0",
        "johnkary/phpunit-speedtrap": "~4.0.0",
        "nelmio/alice": "~3.9.0",
        "oro/twig-inspector": "1.0.*",
        "phpmd/phpmd": "~2.11.1",
        "phpunit/phpunit": "~9.5.1",
        "sebastian/phpcpd": "6.0.*",
        "squizlabs/php_codesniffer": "~3.6.0",
        "symfony/browser-kit": "~5.4.0",
        "symfony/css-selector": "~5.4.0",
        "symfony/error-handler": "~5.4.0",
        "symfony/debug-bundle": "~5.4.0",
        "symfony/dom-crawler": "~5.4.0",
        "symfony/phpunit-bridge": "~5.4.0",
        "symfony/stopwatch": "~5.4.0",
        "symfony/var-dumper": "~5.4.0",
        "symfony/var-exporter": "~5.4.0",
        "symfony/web-profiler-bundle": "~5.4.0",
        "theofidry/alice-data-fixtures": "~1.4.0"
    },
    "config": {
        "bin-dir": "bin",
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "php-http/discovery": false,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@set-permissions",
            "@install-assets",
            "@set-assets-version"
        ],
        "post-update-cmd": [
            "@set-permissions",
            "@update-assets",
            "@set-assets-version"
        ],
        "set-permissions": [
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setPermissions"
        ],
        "install-assets": [
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "update-assets": [
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::updateAssets"
        ],
        "set-assets-version": [
            "Oro\\Bundle\\InstallerBundle\\Composer\\ScriptHandler::setAssetsVersion"
        ],
        "set-parameters": [
            "Oro\\Bundle\\InstallerBundle\\Composer\\ParametersHandler::set"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "require": "~5.4.0"
        },
        "runtime": {
            "dotenv_path": ".env-app",
            "env_var_name": "ORO_ENV",
            "debug_var_name": "ORO_DEBUG",
            "prod_envs": ["prod", "behat_test"]
        },
        "symfony-web-dir": "public",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests"
    }
}
